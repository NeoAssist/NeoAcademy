{
  "name": "neoacademy-front",
  "version": "1.0.0",
  "description": "NeoAcademy front-end",
  "author": "Rodolfo Olivieri",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "storybook": "start-storybook -p 6060",
    "build-storybook": "build-storybook -c .storybook"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^3.4.3",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-notes": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/addon-a11y": "^5.3.14",
    "@storybook/addon-backgrounds": "^5.3.14",
    "@storybook/addon-centered": "^5.3.14",
    "@storybook/addon-contexts": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addon-options": "^5.3.14",
    "@storybook/addon-storyshots": "^5.3.14",
    "@storybook/addon-storysource": "^5.3.14",
    "@storybook/addon-viewport": "^5.3.14",
    "@storybook/vue": "^5.3.14",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^10.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "stylelint": "^10.1.0",
    "vue-jest": "^4.0.0-0",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11"
  }
}
